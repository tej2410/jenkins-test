pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
               bat "rmdir  /s /q test"
                bat "git clone https://github.com/durgesh3326/test.git"
                bat "mvn clean -test"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f test"
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f TicketBookingServiceJunitTesting"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f TicketBookingServiceJunitTesting"
            }
        }
    }
}
